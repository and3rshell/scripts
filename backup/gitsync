#!/usr/bin/env bash

cd "$HOME/.local/scripts" || exit 1
git add .
git commit -m "sync"
git push

cd "$HOME/.local/work" || exit 1
git add .
git commit -m "sync"
git push

cd "$HOME/.local/web" || exit 1
git add .
git commit -m "sync"
git push

cd "$HOME/.config/nvim" || exit 1
git add .
git commit -m "sync"
git push
